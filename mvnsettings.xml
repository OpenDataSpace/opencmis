<?xml version="1.0" encoding="UTF-8"?>
<settings xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd" xmlns="http://maven.apache.org/SETTINGS/1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <!--
         Maven settings for using artifactory.fritz-elfert.de
         In order to use this, you MUST have a valid client-certificate.
         Usage:
            mvn -Djavax.net.ssl.keyStore=/path/to/clientcert.p12 \
                -Djavax.net.ssl.keyStoreType=pkcs12 \
                -Djavax.net.ssl.keyStorePassword=YourCertPassword \
                -s mvnsettings.xml YourMavenGoal
    -->
    <mirrors>
        <mirror>
            <mirrorOf>*</mirrorOf>
            <name>remote-repos</name>
            <url>https://artifactory.fritz-elfert.de/artifactory/remote-repos</url>
            <id>remote-repos</id>
        </mirror>
    </mirrors>

    <profiles>
        <profile>
            <repositories>
                <repository>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                    <id>central</id>
                    <name>libs-release</name>
                    <url>https://artifactory.fritz-elfert.de/artifactory/libs-release</url>
                </repository>
                <repository>
                    <snapshots />
                    <id>snapshots</id>
                    <name>libs-snapshot</name>
                    <url>https://artifactory.fritz-elfert.de/artifactory/libs-snapshot</url>
                </repository>
            </repositories>
            <pluginRepositories>
                <pluginRepository>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                    <id>central</id>
                    <name>plugins-release</name>
                    <url>https://artifactory.fritz-elfert.de/artifactory/plugins-release</url>
                </pluginRepository>
                <pluginRepository>
                    <snapshots />
                    <id>snapshots</id>
                    <name>plugins-snapshot</name>
                    <url>https://artifactory.fritz-elfert.de/artifactory/plugins-snapshot</url>
                </pluginRepository>
            </pluginRepositories>
            <properties>
                <snapshot.deploy.id>artifactory.fritz-elfert.de</snapshot.deploy.id>
                <snapshot.deploy.name>artifactory.fritz-elfert.de-snapshots</snapshot.deploy.name>
                <snapshot.deploy.url>https://artifactory.fritz-elfert.de/artifactory/libs-snapshot-local</snapshot.deploy.url>
            </properties>
            <id>artifactory</id>
        </profile>
    </profiles>
    <activeProfiles>
        <activeProfile>artifactory</activeProfile>
    </activeProfiles>
</settings>
